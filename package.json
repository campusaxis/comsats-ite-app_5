    "audit:performance": "lighthouse http://localhost:3000 --only-categories=performance --chrome-flags='--no-sandbox'",
    "audit:seo": "lighthouse http://localhost:3000 --only-categories=seo --chrome-flags='--no-sandbox'",
    "audit:all": "npm run audit:accessibility && npm run audit:performance && npm run audit:seo",
