{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build Next.js app",
			"type": "shell",
			"command": "pnpm",
			"args": [
				"build"
			],
			"isBackground": false,
			"problemMatcher": [
				"$tsc"
			],
			"group": "build"
		},
		{
			"label": "Build with npm",
			"type": "shell",
			"command": "npm",
